# ThetaData Credentials
THETA_USERNAME=your_email@example.com
THETA_PASSWORD=your_password_here

# Service Configuration
THETA_JAR_PATH=./ThetaTerminal.jar
LOG_LEVEL=INFO

# Historical Stock Data Configuration
STOCK_TIER=value
# Options: value, standard, pro
# value tier uses utp_ca venue for quotes, others use nqb

# MinIO Object Storage Configuration
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123
MINIO_BUCKET=betedge-data
MINIO_SECURE=false
MINIO_REGION=us-east-1

# Application Configuration
FORCE_REFRESH=false