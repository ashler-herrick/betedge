# ThetaData Credentials
THETA_USERNAME=your_email@example.com
THETA_PASSWORD=your_password_here

# Service Configuration
THETA_JAR_PATH=./ThetaTerminal.jar
LOG_LEVEL=INFO

# Historical Stock Data Configuration
STOCK_TIER=value
# Options: value, standard, pro
# value tier uses utp_ca venue for quotes, others use nqb

# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=localhost:9092

# Optional: Override default Kafka topics
# KAFKA_HISTORICAL_STOCK_TOPIC=historical-stock-data
# KAFKA_HISTORICAL_OPTION_TOPIC=historical-option-data
# KAFKA_LIVE_STOCK_TOPIC=live-stock-data
# KAFKA_LIVE_OPTION_TOPIC=live-option-data
# KAFKA_ERROR_TOPIC=data-processing-errors

# Optional: Kafka producer settings
# KAFKA_MAX_RETRIES=3
# KAFKA_FLUSH_TIMEOUT=10.0